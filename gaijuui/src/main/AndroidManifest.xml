<?xml version="1.0" encoding="utf-8"?><!-- androidbootstrapの設定ファイル(変更の必要なし) -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.gaijuui">

    <application
        android:name=".TestBootstrap"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="はたけまもる"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GaijuActivity"
            android:screenOrientation="portrait"
            android:label="はたけまもる"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataActivity"
            android:label="統計データ"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name=".ConfActivity"
            android:screenOrientation="portrait"
            android:label="設定"></activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".NetworkActivity"
            android:screenOrientation="portrait"
            android:label="ネットワーク設定"></activity>
    </application>

</manifest>